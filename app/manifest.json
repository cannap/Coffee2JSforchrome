{
  "name": "__MSG_appName__",
  "version": "0.0.8",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://github.com/*"
  ],
  "background": {
    "scripts": [
      "scripts/chromereload.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*"
      ],
      "js": [
        "scripts/jquery.js",
        "scripts/highlight.js",
        "scripts/coffee-script.js",
        "scripts/contentscript.js"
      ],
      "css": [
        "styles/style.css"
      ],
      "run_at": "document_end"
    }
  ]
}